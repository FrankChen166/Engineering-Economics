<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Register</title>
  </head>
  <body>
    <form action="/register" method="POST">
      <div>
        <label for="user">Enter username</label>
        <input
          type="text"
          id="username"
          name="username"
          placeholder="username"
        />
      </div>
      <div>
        <label for="studentId">Enter studentId</label>
        <input
          type="text"
          id="studentId"
          name="studentId"
          placeholder="studentId"
        />
      </div>
      <div>
        <label for="useraccount">Enter useraccount</label>
        <input
          type="text"
          id="useraccount"
          name="useraccount"
          placeholder="useraccount"
        />
      </div>
      <div>
        <label for="password">Enter password</label>
        <input
          type="password"
          id="password"
          name="password"
          placeholder="password"
        />
      </div>
      <button>sign up</button>
    </form>
  </body>
</html>
